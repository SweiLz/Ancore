<?xml version="1.0"?>
<launch>
    <node pkg="ancore" type="ancore_node" name="ancore_node" output="screen">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baudrate" value="115200"/>

        <param name="wheel_separation" value="0.65"/>
        <param name="wheel_radius" value="0.08"/>

        <param name="publish_odom_frame" value="true"/>

        <param name="imu_frame_id" value="imu_link"/>
        <param name="imu_topic" value="imu"/>
        <param name="odom_frame_id" value="odom"/>
        <param name="odom_topic" value="odom"/>
        <param name="base_frame_id" value="base_footprint"/>
        <param name="cmd_topic" value="cmd_vel"/>
    </node>
</launch>